{
    "manifest_version":2,
	"name": "Aura Inspector",
	"version": "0.74",
	"description": "For inspecting Aura apps",
	"update_url": "http://download.auraframework.org/aura-inspector/aura-inspector-updates.xml",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"background": {
        "scripts" : [ "background.js" ]
    },
    "web_accessible_resources": [
    	"AuraInspectorInjectedScript.js"
    ],
	"devtools_page": "devtools.html",
	"content_scripts": [{
        "matches": ["*://*/*cmp*", "*://*/*app*"],
		// "matches": [
		// 	"http://localhost/*",
		// 	"http://*.salesforce.com/*",
		// 	"http://*.force.com/*"
		// ],
		"js": ["contentScript.js"],
		"run_at": "document_start",
		"all_frames": false
	}],
	"permissions": [
		"tabs", 
		"background"
	],

	// For development only. Generates a specific extensionId for the Aura Inspector, so the Sfdc Inspector can reliably send messages do it.
	"key": ""
}
