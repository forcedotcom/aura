{
    "extends" : [ "plugin:lwc/recommended" ],
    "plugins" : [ "lwc" ],
    "rules" : {
        "lwc/no-inline-disable" : [
                1,
                {
                    "whitelist" : {
                        "lwc/no-set-timeout" : [ "**/aura-modules/src/test/modules/moduletest/async/async.js",
                            "**/aura-modules/src/test/modules/securemoduletest/sanityChecks/sanityChecks.js"],
                        "dot-notation" : [ "**/aura-modules/src/test/modules/moduletest/exports/exports.js" ],
                        "no-console" : [ "**/src/test/modules/**/*.js" ],
                        "no-unused-vars" : [ "**/src/main/modules/module/marker/marker.js",
                            "**/src/test/modules/**/*.js",
                            "**/src/test/schemas/**/*.js" ],
                        "lwc/no-compat" : [ "**/aura-modules/src/test/modules/moduletest/importCmp/importCmp.js" ],
                        "lwc/no-compat-execute" : [ "**/aura-modules/src/test/modules/moduletest/exports/exports.js" ],
                        "lwc/no-compat-module-storage" : [ "**/aura-modules/src/test/modules/moduletest/importCmp/importCmp.js" ],
                        "lwc/no-aura-libs" : [ "**/aura-modules/src/test/modules/moduletest/simpleCmp/simpleCmp.js" ],
                        "lwc/no-aura" : [ "**/aura-modules/src/test/modules/securemoduletest/bootstrap/bootstrap.js",
                            "**/aura-modules/src/test/modules/securemoduletest/sanityChecks/sanityChecks.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simpleCmp/simpleCmp.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simpleLib/simpleLib.js",
                            "**/aura-modules/src/test/modules/securemoduletest/sanityChecks/helper.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simpleCmp/simpleCmp.js",
                            "**/aura-modules/src/test/modules/securemoduletest/componentLifecycleTester/componentLifecycleTester.js",
                            "**/aura-modules/src/test/modules/securemoduletest/testConstructor/testConstructor.js"],
                        "no-undef" : [ "**/aura-modules/src/test/modules/securemoduletest/bootstrap/bootstrap.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simpleCmp/simpleCmp.js",
                            "**/aura-modules/src/test/modules/securemoduletest/simpleLib/simpleLib.js"],
                        "no-script-url" : [ "**/aura-modules/src/test/modules/moduletest/navItem/navItem.js"],
                        "lwc/no-inner-html" : ["**/aura-modules/src/test/modules/securemoduletest/sanityChecks/helper.js"],
                        "no-alert" : ["**/aura-modules/src/test/modules/securemoduletest/sanityChecks/sanityChecks.js"],
                        "no-array-constructor": ["**/aura-modules/src/test/modules/securemoduletest/sanityChecks/sanityChecks.js"],
                        "no-new-object": ["**/aura-modules/src/test/modules/securemoduletest/sanityChecks/sanityChecks.js"]
                    }
                } ]
    }
}
