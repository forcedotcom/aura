                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-antrun-plugin</artifactId>
                    <version>1.6</version>
                    <executions>
                        <execution>
                            <id>index-test-components</id>
                            <phase>disabled</phase>
                            <configuration>
                                <target>
                                    <path id="definitions">
                                        <fileset dir="src/test/components">
                                            <include name="**/*.cmp" />
                                            <include name="**/*.intf" />
                                            <include name="**/*.evt" />
                                            <include name="**/*.app" />
                                            <include name="**/*.css" />
                                            <include name="**/*.theme" />
                                            <include name="**/*Test.js" />
                                        </fileset>
                                    </path>
                                    <pathconvert property="definitionList" refid="definitions" pathsep=",">
                                        <regexpmapper from=".*/([^/]*)/[^/]*/(.*)" to="\1:\2" />
                                    </pathconvert>
                                    <echo file="target/src-gen/test/components/.index">${definitionList}</echo>
                                </target>
                            </configuration>
                            <goals>
                                <goal>run</goal>
                            </goals>
                        </execution>
                        <execution>
                            <id>index-components</id>
                            <phase>disabled</phase>
                            <configuration>
                                <target>
                                    <path id="definitions">
                                        <fileset dir="src/main/components">
                                            <include name="**/*.cmp" />
                                            <include name="**/*.intf" />
                                            <include name="**/*.evt" />
                                            <include name="**/*.app" />
                                            <include name="**/*.css" />
                                            <include name="**/*.theme" />
                                            <include name="**/*Test.js" />
                                        </fileset>
                                    </path>
                                    <pathconvert property="definitionList" refid="definitions" pathsep=",">
                                        <regexpmapper from=".*/([^/]*)/[^/]*/(.*)" to="\1:\2" />
                                    </pathconvert>
                                    <echo file="target/src-gen/main/components/.index">${definitionList}</echo>
                                </target>
                            </configuration>
                            <goals>
                                <goal>run</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <groupId>org.mortbay.jetty</groupId>
                    <artifactId>jetty-maven-plugin</artifactId>
                    <version>8.1.4.v20120524</version>
                    <dependencies>
                        <dependency>
                            <groupId>org.slf4j</groupId>
