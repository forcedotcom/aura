    <build>
        <testResources>
            <testResource>
                <directory>src/test</directory>
                <includes>
                    <include>results/**/*</include>
                    <include>testdata/**/*</include>
                </includes>
            </testResource>
        </testResources>
        <plugins>
            <plugin>
                <inherited>false</inherited>
                <groupId>com.mycila.maven-license-plugin</groupId>
                <artifactId>maven-license-plugin</artifactId>
                <version>1.9.0</version>
                <configuration>
                    <strictCheck>true</strictCheck>
                    <mapping>
                        <java>SLASHSTAR_STYLE</java>
                        <groovy>JAVADOC_STYLE</groovy>
                        <js>SLASHSTAR_STYLE</js>
                        <css>SLASHSTAR_STYLE</css>
                        <xml>XML_STYLE</xml>
                        <dtd>XML_STYLE</dtd>
                        <xsd>XML_STYLE</xsd>
                        <html>XML_STYLE</html>
                        <htm>XML_STYLE</htm>
                        <xsl>XML_STYLE</xsl>
                        <fml>XML_STYLE</fml>
                        <cmp>XML_STYLE</cmp>
                        <app>XML_STYLE</app>
                        <intf>XML_STYLE</intf>
                        <evt>XML_STYLE</evt>
                        <apt>DOUBLETILDE_STYLE</apt>
                        <properties>SCRIPT_STYLE</properties>
                        <sh>SCRIPT_STYLE</sh>
                        <txt>TEXT</txt>
                        <bat>BATCH</bat>
                        <cmd>BATCH</cmd>
                        <sql>DOUBLEDASHES_STYLE</sql>
                        <jsp>DYNASCRIPT_STYLE</jsp>
                        <ftl>FTL</ftl>
                        <xhtml>XML_STYLE</xhtml>
                        <vm>SHARPSTAR_STYLE</vm>
                        <jspx>XML_STYLE</jspx>
                    </mapping>
                    <header>${aura.home}/aura/src/test/resources/header.txt</header>
                    <failIfMissing>true</failIfMissing>
                    <aggregate>true</aggregate>
                    <properties>
                        <owner>salesforce.com, inc.</owner>
                        <year>2013</year>
                    </properties>
                    <includes>
                        <include>**/src/main/java/**</include>
                        <include>**/src/main/javascript/**</include>
                        <include>**/src/main/resources/**/*.js</include>
                        <include>**/src/main/components/**</include>
                        <include>**/src/test/javascript/**</include>
                        <include>**/src/test/java/**</include>
                        <include>**/src/test/resources/**/*.js</include>
                    </includes>
                    <excludes>
                        <exclude>LICENSE.txt</exclude>
                        <exclude>**/src/test/resources/**</exclude>
                        <exclude>**/src/test/data/**</exclude>
                        <exclude>**/.gitignore</exclude>
                        <exclude>**/.index</exclude>
                        <exclude>**/version.prop</exclude>
                        <exclude>aura-resources/src/main/resources/**</exclude>
                        <exclude>**/target/**</exclude>
                    </excludes>
                </configuration>
                <executions>
                    <execution>
                        <id>check-headers</id>
                        <phase>verify</phase>
                        <goals>
                            <goal>check</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-source-plugin</artifactId>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>attach-test-sources</id>
                        <goals>
                            <goal>test-jar</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <attach>true</attach>
                </configuration>
            </plugin>
